<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/findmebg"
    android1:gravity="clip_vertical" >


	<ImageView
		android1:src="@drawable/findmebtnnormal"
		android1:id="@+id/ivAcerca"
		android1:layout_width="wrap_content"
		android1:layout_height="wrap_content"

	/>

    <TextView
        android1:id="@+id/tVAcerca"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_centerHorizontal="true"
        android1:layout_marginBottom="60dp"
        android1:text="@string/acercade"
        android1:textAppearance="?android:attr/textAppearanceSmall"
        android1:textColor="#ffffff"
        android1:textSize="@dimen/tam15"
        android1:textStyle="bold" />

    <Button
        android1:id="@+id/btnRegresar"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentBottom="true"
        android1:layout_centerHorizontal="true"
        android1:layout_marginBottom="40dp"
        android1:onClick="onRegresarClick"
        android1:text="@string/regresar"
        android1:textColor="#ffffff"
        android1:textSize="18sp"
        android1:textStyle="normal|bold" />

</RelativeLayout>
